{
    "manifest_version": 3,
    "name": "Simkl2PlexDev",
    "description": "Sync Simkl watch history to your plex account: development version",
    "version": "0.0.1",
    "background": {
        "service_worker": "/js/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "webNavigation"
    ]
}
